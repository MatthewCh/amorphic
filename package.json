{
  "name": "amorphic",
  "description": "Front to back isomorphic framework for developing applications with node.js and mongoDB",
  "homepage": "https://github.com/selsamman/amorphic",
  "version": "1.5.3",
  "dependencies": {
    "q": "1.x",
    "supertype": "git://github.com/nikmash/supertype.git",
    "semotus": "git://github.com/nikmash/semotus.git",
    "persistor": "git://github.com/nikmash/persistor.git",
    "amorphic-bindster": "git://github.com/nikmash/amorphic-bindster.git#refactor",
    "mongodb-bluebird": "x",
    "formidable": "1.x",
    "nconf": "0.x",
    "connect": "2.6.2",
    "node-mandrill": "1.x",
    "uglify-js": "x",
    "compression": "x"
  },
  "devDependencies": {
    "axios": "^0.15.2",
    "bluebird": "^3.4.6",
    "chai": "3.x",
    "eslint": "3.7.x",
    "form-data": "^2.1.2",
    "knex": "x",
    "istanbul": "0.4.5",
    "mocha": "^3.1.2",
    "mock-fs": "3.6.x",
    "pg": "x",
    "request": "x",
    "restify": "2.x",
    "sinon": "^1.17.6",
    "underscore": "1.x",
    "xmlhttprequest-cookie": "x"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "istanbul cover _mocha -- test/* -R spec",
